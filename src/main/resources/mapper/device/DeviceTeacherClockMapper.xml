<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cqyz.aibeijiayuan.api.device.mapper.DeviceTeacherClockMapper">

    <select id="getClockByMsAndId" resultType="com.cqyz.aibeijiayuan.bean.clock.po.TeacherClock">
        select * from abjy_tb_teacher_clock where is_delete = 0 and teacher_id = #{teacherId} and clock_millisecond = #{clockMillisecond} limit 1
    </select>
</mapper>