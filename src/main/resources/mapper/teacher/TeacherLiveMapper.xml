<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cqyz.aibeijiayuan.api.customer.mapper.TeacherLiveMapper">

    <select id="getLivesNeedNotice" resultType="com.cqyz.aibeijiayuan.bean.live.po.Live">
        select * from abjy_tb_live
        where is_delete = 0 and is_push = 1
        and push_status = 0 and live_status = 0
        and TIMESTAMPDIFF(minute,live_start_time,NOW()) &lt; -15
    </select>

    <select id="getList" resultType="com.cqyz.aibeijiayuan.bean.live.vo.LiveVO">


    </select>
</mapper>